LiquidCrystal lcd(rs, en, d4, d5, d6, d7);
Servo myservo;

long distance, duration;
int trigPin = 9;
int echoPin = 8;

int pinMelody = 7;
int nextNote = 0;
int szMelody = sizeof(melody) / sizeof(int);
int pos = 0;

void setup() {
  lcd.begin(16, 2);
  pinMode(trigPin, OUTPUT);
  pinMode(echoPin, INPUT);
  pinMode(pinMelody, OUTPUT);
  myservo.attach(9);
  Serial.begin(9600);
}

void playNextNote(int thisNote) {
    tone(pinMelody, melody[thisNote], noteDurations[thisNote] * .7);    
    delay(noteDurations[thisNote]);    
    noTone(pinMelody);
}

void loop() {
    lcd.setCursor(0, 1);
    digitalWrite(trigPin, LOW);
    delayMicroseconds(2);

    digitalWrite(trigPin, HIGH);
    delayMicroseconds(10);

    digitalWrite(trigPin, LOW);

    duration = pulseIn(echoPin, HIGH);

    distance = duration/58.2;

    Serial.println(distance);

    delay(50);

  if (distance >= 50) {
    lcd.print("You are so far!");
  }

  if (distance >= 21 && distance < 50) {
    lcd.print("You are far!!!!");
  }

  if (distance <= 20 && distance > 10) {
    lcd.print("You are close!");
  }

  if (distance == 19) {
    startPlayback(shaunvoice, sizeof(shaunvoice));
  }

  if (distance <= 8) {
    lcd.print("You are here!");
    myservo.write(pos);
    playNextNote(nextNote);
    if (nextNote < szMelody) {
      nextNote++;
      } else {
        nextNote = 0;
      }
  }


  if (distance > 8) {
    noTone(pinMelody);
    nextNote = 0;
  }
}
